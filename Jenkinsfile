pipeline {
    agent any

    stages {

        stage ("Checkout") {
            steps {
                echo "Checking out file from Github"
            }
        }

        stage ("Build") {
            steps {
                echo "Building Stage"
            }
        }

        stage ("Test") {
            steps {
                echo "Testing Stage"
            }
        }

        stage ("Deploy") {
            steps {
                echo "Deployment Stage"
            }
        }
    }
}